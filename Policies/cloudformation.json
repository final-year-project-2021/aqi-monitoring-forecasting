{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Action":[
                "apigateway:*",
                "codedeploy:*",
                "lambda:*",
                "dynamodb:*",
                "cloudformation:CreateChangeSet",
                "iam:GetRole",
                "iam:CreateRole",
                "iam:DeleteRole",
                "iam:PutRolePolicy",
                "iam:AttachRolePolicy",
                "iam:DeleteRolePolicy",
                "iam:DetachRolePolicy",
                "iam:PassRole",
                "s3:GetObject",
                "s3:GetObjectVersion",
                "s3:GetBucketVersioning"
            ],
            "Resource":"*",
            "Effect":"Allow"
        }
    ]
}